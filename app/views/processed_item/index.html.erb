<div id="content" class="col-md-10">

  <section class="page-header">
    <h2>Processed Items</h2>
  </section>

  <section class="queue">
    <div class="paging">
      <%= paginate @items %>
    </div>
    <table class="table">
      <thead>
      <tr>
        <th>Name</th>
        <th>Etag</th>
        <th>Bag Date</th>
        <th>Bucket</th>
        <th>User</th>
        <th>Institution</th>
        <th>Date</th>
        <th>Note</th>
        <th>Action</th>
        <th>Stage</th>
        <th>Status</th>
        <th>Outcome</th>
      </tr>
      </thead>
      <tbody>
      <% @items.each do |item| %>
          <tr>
            <td><%= item.name %></td>
            <td><%= item.etag %></td>
            <td><%= item.bag_date %></td>
            <td><%= item.bucket %></td>
            <td><%= item.user %></td>
            <td><%= item.institution %></td>
            <td><%= item.date %></td>
            <td><%= item.note %></td>
            <td><%= item.action %></td>
            <td><%= item.stage %></td>
            <td><%= item.status %></td>
            <td><%= item.outcome %></td>
          </tr>
      <% end %>
      </tbody>
    </table>
  </section>
</div>