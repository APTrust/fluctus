<div id="content" class="col-md-10">

  <section class="page-header">
    <h2>Processed Items</h2>
  </section>

  <section class="queue">
    <div class="paging">
      <%= paginate @items %>
    </div>
    <table class="table">
      <thead>
      <tr>
        <th>Name</th>
        <th>Etag</th>
        <th>Institution</th>
        <th>Processing Date</th>
        <th>Action</th>
        <th>Stage</th>
        <th>Status</th>
      </tr>
      </thead>
      <tbody>
      <% @items.each do |item| %>
          <tr class="<%= item.status %>">
            <td><a href="<%= processed_item_path(id: item.id) %>"><%= item.name %></a></td>
            <td><%= item.etag %></td>
            <td><%= item.institution %></td>
            <td><%= item.date %></td>
            <td><%= item.action %></td>
            <td><%= item.stage %></td>
            <td><%= item.status %></td>
          </tr>
      <% end %>
      </tbody>
    </table>
  </section>
</div>
