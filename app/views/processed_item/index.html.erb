<div id="content" class="col-md-10">
  <ol class="breadcrumb">
    <li><a href="#"><%= link_to current_user.institution.name, current_user.institution %></a></li>
  </ol>

  <section class="page-header">
    <h2>Processed Items</h2>
  </section>

  <section class="queue">
    <table class="table">
      <thead>
      <tr>
        <th>Name</th>
        <th>Etag</th>
        <th>Processing Date</th>
        <th>Action</th>
        <th>Stage</th>
        <th>Status</th>
      </tr>
      </thead>
      <tbody>
      <% @processed_items.each do |item| %>
          <tr class="<%= item.status %>">
            <td><a href="<%= processed_item_path(id: item.id) %>"><%= item.name %></a></td>
            <td><%= item.etag %></td>
            <td><%= item.date %></td>
            <td><%= item.action %></td>
            <td><%= item.stage %></td>
            <td><%= item.status %></td>
          </tr>
      <% end %>
      </tbody>
    </table>
  </section>
</div>
